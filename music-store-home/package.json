{
  "name": "music-store-home",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project music-store-home --port 3000",
    "start:albums": "ng serve --project music-store-albums --port 3001",
    "start:events": "ng serve --project music-store-events --port 3002",
    "build": "npm run build:clients && npm run build:music-store-home",
    "build:clients": "npm run build:music-store-albums && npm run build:music-store-events",
    "build:music-store-albums": "ng build --prod --single-bundle --project music-store-albums --output-hashing none --vendor-chunk false --output-path src/assets/micro-frontends/music-store-albums",
    "build:music-store-events": "ng build --prod --single-bundle --project music-store-events --output-hashing none --vendor-chunk false --output-path src/assets/micro-frontends/music-store-events",
    "build:music-store-home": "ng build --project music-store-home",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node copy-wc-polyfill.js && node copy-bundles.js && node projects/music-store-albums/copy-bundles.js && node projects/music-store-events/copy-bundles.js",
    "build:externals": "ng build --extra-webpack-config webpack.externals.js --prod ",
    "build:music-store-home:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project music-store-home ",
    "build:music-store-albums:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project music-store-albums --single-bundle --output-path src/assets/micro-frontends/music-store-albums",
    "build:music-store-events:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project music-store-events --single-bundle --output-path src/assets/micro-frontends/music-store-events",
    "build:all:externals": "npm run build:music-store-albums:externals && npm run build:music-store-events:externals && npm run build:music-store-home:externals"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/elements": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/router": "~8.0.1",
    "@webcomponents/custom-elements": "^1.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "bootstrap": "^3.3.7",
    "copy": "^0.3.2",
    "core-js": "^2.5.4",
    "ngx-build-plus": "^8.1.1",
    "rxjs": "6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "query-selector-shadow-dom": "^0.6.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.800.3",
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "^8.0.3",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "cpr": "^3.0.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.4.5",
    "webpack-cli": "^2.0.14"
  }
}
